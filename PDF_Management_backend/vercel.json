{
  "version": 2,
  "buildCommand": "dotnet publish -c Release",
  "outputDirectory": "PdfManagement.API/bin/Release/net8.0/publish",
  "framework": {
    "version": "8.0"
  },
  "builds": [
    {
      "src": "PdfManagement.API/PdfManagement.API.csproj",
      "use": "@vercel/dotnet"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "PdfManagement.API/PdfManagement.API.csproj"
    }
  ]
}
